<h2><%= assignment.name %></h2>

<div>
    <embed src="<%= assignment.assignpdfPath %>" type="application/pdf" width="100%" height="600px" />
    <%= assignment.deadline.toDateString() %>
    <%= assignment.subject %>
    <% if(assignment.priority === true) { %>
    <h6>Important</h6>
    <%  } %>
</div>
<div>
    <a href="/assignments/<%= assignment.id %>/edit">Edit</a>
    <%- include('../partials/deleteForm', {url: `/assignments/${assignment.id}`}) %>
</div>