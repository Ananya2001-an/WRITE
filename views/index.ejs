<h1>Important Assignments ✏️</h1>
<div>
    <%  assignments.forEach(assignment => { %>
        <% if(assignment.priority === true) { %>
            <div>
            <%= assignment.name %>
            <%= assignment.deadline.toDateString() %>
            <%= assignment.subject %>
            <a href="/assignments/<%= assignment.id %>"><h2>📑</h2></a>
            </div>
        <% } %>
     <% }) %>
     
     
</div>

