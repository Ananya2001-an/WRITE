<div class="container">
<h1 class="page-header">Important Assignments ✏️</h1>
<div class="inner-container">
    <%  assignments.forEach(assignment => { %>
        <% if(assignment.priority === true) { %>
            <div class="index-contents-data">
            <p><span>Name: </span>  <%= assignment.name %></p> 
            <p><span>Subject: </span>  <%= assignment.subject %></p> 
            <p><span>Deadline: </span>  <%= assignment.deadline.toDateString() %></p> 
            <a href="/assignments/<%= assignment.id %>">view 📑</a>
            </div>
        <% } %>
     <% }) %>
     
     
</div>
</div>
