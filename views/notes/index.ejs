<h2>Search Notes</h2>
<form action="/notes" method="get">
    <label>Name:</label>
    <input type="text" name="name" value="<%= searchOptions.name %>">
<button type="submit">Search</button>
<div><% notes.forEach(note => { %>
    <%= note.name %>
    <a href="/notes/<%= note.id %>">View</a>
    <a href="/notes/<%= note.id %>/edit">Edit</a>
    <%- include('../partials/deleteForm', {url: `/notes/${note.id}`}) %>
    
<% }) %>
</div>
</form>